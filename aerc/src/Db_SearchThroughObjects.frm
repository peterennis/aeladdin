Private Sub B_CopyToClipboard_Click()
    
       Dim Result As String
       Dim Col    As Integer
       Dim Col0   As Integer
       Dim ColN   As Integer
       Dim Row    As Long
       Dim str    As String
    
       Col0 = LBound(ResultLst, 1)
      ColN = UBound(ResultLst, 1)
      Result = ""
    
      For Row = LBound(ResultLst, 2) To UBound(ResultLst, 2)
        
          str = ""
        
          For Col = Col0 To ColN
              If str = "" Then
                  If Result <> "" Then
                      str = vbCrLf
                  End If
              Else
                  str = str & vbTab
              End If
              str = str & ResultLst(Col, Row)
          Next Col
        
          Result = Result & str
        
      Next Row
    
