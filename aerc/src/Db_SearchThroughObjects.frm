Private Sub p_Scan()
    
       Dim obj          As Object
       Dim Obj2         As Object
       Dim Obj3         As Object
       Dim ObjWasOpen   As Boolean
       Dim ObjIndex     As Long
       Dim ObjName      As String
    
       Dim i            As Long
      Dim Ok           As Boolean
      Dim InfoTxt      As String
    
'    Initialize
      SearchProcessing = True
      SearchContinue = True
    
      ReplaceNbr = 0
      ReplaceWithStr = vbNullString & ReplaceWith
      SearchStr = vbNullString & SearchWhat
      SearchType = L_SearchType.Value
    
      ResultNbr = 0
    
'    Search through Database
'    -----------------------
      If C_Search_DataBase And SearchContinue Then
        
          p_Info m_GetText("Db_SearchThrougthObjects", "E_C_DataBase") & ": " & g_CurrDb.Name
        
          p_ScanProperties g_CurrDb, c_DB
        
          For Each Obj2 In g_CurrDb.Containers
              For Each Obj3 In Obj2.Documents
                  DoEvents
                  If Not SearchContinue Then Exit For
                  p_ScanProperties Obj3, c_Doc, Obj2.Name, Obj3.Name
              Next Obj3
          Next Obj2
        
          Set Obj2 = Nothing
          Set Obj3 = Nothing
        
      End If
    
'    Search through Tables
'    ----------------------
      If (C_Search_Tables Or C_Search_SysTables) And SearchContinue Then
        
          InfoTxt = m_GetText("Db_SearchThrougthObjects", "E_C_Tables") & ": "
        
          For ObjIndex = 0 To CurrentData.AllTables.Count - 1 ' CurrentData.AllTables.Count is faster than g_TableDefs.Count
            
              ObjName = CurrentData.AllTables(ObjIndex).Name
              Set obj = g_TableDefs(ObjName) ' Caution: TableDefs and AllTables is not the same collection.
              Ok = p_IsSysTable(obj) ' =True if it's a system table
            
              If (C_Search_SysTables And Ok) Or (C_Search_Tables And (Not Ok)) Then
                
                  p_Info InfoTxt & ObjName
                  If Not SearchContinue Then Exit For
                
                  p_ScanProperties obj, c_Tbl, ObjName
                  For Each Obj2 In obj.Fields
                      p_ScanProperties Obj2, c_Tbl, obj.Name, Obj2.Name
                  Next Obj2
'                p_AddInList NoPermLst, NoPermNbr, c_Tbl, Obj.Name, E_Txt_NoPermission.Caption
                
                  p_ReplaceObjectName acTable, ObjName
                
              End If
            
          Next ObjIndex
        
          Set obj = Nothing
          Set Obj2 = Nothing
        
      End If
    
'    Search through Queries
'    ----------------------
      If C_Search_Queries And SearchContinue Then
        
          InfoTxt = m_GetText("Db_SearchThrougthObjects", "E_C_Queries") & ": "
        
          For ObjIndex = 0 To CurrentData.AllQueries.Count - 1 ' CurrentData.AllQueries.Count is faster than g_QueryDefs.Count
            
              ObjName = CurrentData.AllQueries(ObjIndex).Name
              Set obj = g_QueryDefs(ObjName) ' Caution: QueryDefs and AllQueries is not the same collection. They possibly have not the same items because QueryDefs has also queries saved in forms
              p_Info InfoTxt & ObjName
              If Not SearchContinue Then Exit For
            
              p_ScanProperties obj, c_Qry, ObjName
'            p_AddInList NoPermLst, NoPermNbr, c_Qry, ObjName, E_Txt_NoPermission.Caption
            
              p_ReplaceObjectName acQuery, ObjName
            
          Next ObjIndex
        
          Set obj = Nothing
        
     End If
    
'    Search through Forms
'    ----------------------
     If C_Search_Forms Or C_Search_FormModules Then
         If SearchContinue Then
            
             InfoTxt = m_GetText("Db_SearchThrougthObjects", "E_C_Forms") & ": "
            
             For ObjIndex = 0 To CurrentProject.AllForms.Count - 1
                
                 ObjName = CurrentProject.AllForms(ObjIndex).Name
                 Ok = m_CurrProj_ObjOpen(acForm, ObjName, ObjWasOpen, obj)
                 p_Info InfoTxt & ObjName
                 If Not SearchContinue Then Exit For
                
                 If Ok Then
                    
                     If C_Search_Forms Then
                        
                         p_ScanProperties obj, c_Frm, ObjName
                        
'                        controls
                         For Each Obj2 In obj.Controls
                             p_ScanProperties Obj2, c_Frm, ObjName, Obj2.Name
                             If p_HasFormatConditions(Obj2) Then
                                 For i = 0 To Obj2.FormatConditions.Count - 1
                                     If p_Search(Obj2.FormatConditions(i).Expression1) Or p_Search(Obj2.FormatConditions(i).Expression2) Then
                                         p_AddInList ResultLst, ResultNbr, c_Frm, ObjName, Obj2.Name, ".FormatConditions(" & i + 1 & ")"
                                         p_ReplaceFormatCondition Obj2, acForm, ObjName, i
                                     End If
                                 Next
                             End If
                         Next Obj2
                        
'                        sections
                         On Error Resume Next
                         For i = 0 To 4
                             p_ScanProperties obj.Section(i), c_Frm, ObjName, obj.Section(i).Name
                         Next i
                         On Error GoTo 0
                        
                     End If
                    
'                    module
                     If C_Search_FormModules Then
                         If obj.HasModule Then
                             p_ScanModule obj.Module
                         End If
                     End If
                    
                     If ObjWasOpen = False Then
                         DoCmd.Close acForm, ObjName
                     End If
                    
                 Else
                     p_AddInList NoPermLst, NoPermNbr, c_Frm, ObjName, E_Txt_NoPermission.Caption
                 End If
                
                 p_ReplaceObjectName acForm, ObjName
                
             Next ObjIndex
            
             Set obj = Nothing
             Set Obj2 = Nothing
            
         End If
        
     End If
    
'    Search through Reports
'    ----------------------
     If (C_Search_Reports Or C_Search_ReportModules) And SearchContinue Then
        
         InfoTxt = m_GetText("Db_SearchThrougthObjects", "E_C_Reports") & ": "
        
         For ObjIndex = 0 To CurrentProject.AllReports.Count - 1
            
             ObjName = CurrentProject.AllReports(ObjIndex).Name
             Ok = m_CurrProj_ObjOpen(acReport, ObjName, ObjWasOpen, obj)
             p_Info InfoTxt & ObjName
             If Not SearchContinue Then Exit For
            
             If Ok Then
                
                 If C_Search_Reports Then
                    
                     p_ScanProperties obj, c_Rpt, ObjName
                    
'                    controls
                     For Each Obj2 In obj.Controls
                         p_ScanProperties Obj2, c_Rpt, ObjName, Obj2.Name
                         If p_HasFormatConditions(Obj2) Then
                             For i = 0 To Obj2.FormatConditions.Count - 1
                                 If p_Search(Obj2.FormatConditions(i).Expression1) Or p_Search(Obj2.FormatConditions(i).Expression2) Then
                                     p_AddInList ResultLst, ResultNbr, c_Rpt, ObjName, Obj2.Name, ".FormatConditions(" & i + 1 & ")"
                                     p_ReplaceFormatCondition Obj2, acReport, ObjName, i
                                 End If
                             Next
                         End If
                     Next Obj2
                    
'                    sections
                     On Error Resume Next
                     For i = 0 To 24
                         p_ScanProperties obj.Section(i), c_Rpt, ObjName, obj.Section(i).Name
                     Next i
                     On Error GoTo 0
                    
'                    group level
                     On Error Resume Next
                     For i = 0 To 9
                         p_ScanProperties obj.GroupLevel(i), c_Rpt, ObjName, "Group Level " & i
                     Next i
                     On Error GoTo 0
                    
                 End If
                
'                module
                 If C_Search_ReportModules Then
                     If obj.HasModule Then
                         p_ScanModule obj.Module
                     End If
                 End If
                
                 If ObjWasOpen = False Then
                     DoCmd.Close acReport, ObjName
                 End If
                
             Else
                 p_AddInList NoPermLst, NoPermNbr, c_Rpt, ObjName, E_Txt_NoPermission.Caption
             End If
            
             p_ReplaceObjectName acReport, ObjName
            
         Next ObjIndex
        
         Set obj = Nothing
         Set Obj2 = Nothing
        
     End If
    
'    Search through Macros
'    ----------------------
     If C_Search_Macros And SearchContinue Then
        
         InfoTxt = m_GetText("Db_SearchThrougthObjects", "E_C_Macros") & ": "
        
         For ObjIndex = 0 To CurrentProject.AllMacros.Count - 1
            
             ObjName = CurrentProject.AllMacros(ObjIndex).Name
             p_Info InfoTxt & ObjName
             If Not SearchContinue Then Exit For
            
             If oMac.OpenMacro(ObjName) Then
                 p_ScanOpenMacro
             Else
                 p_AddInList NoPermLst, NoPermNbr, c_Mac, ObjName, E_Txt_NoPermission.Caption
             End If
            
             p_ReplaceObjectName acMacro, ObjName '=> I don't know how to performe a replace action in a Macro :(
            
         Next ObjIndex
        
     End If
    
'    Search through Modules
'    ----------------------
     If C_Search_Modules And SearchContinue Then
        
         InfoTxt = m_GetText("Db_SearchThrougthObjects", "E_C_Modules") & ": "
        
         For ObjIndex = 0 To CurrentProject.AllModules.Count - 1
            
             ObjName = CurrentProject.AllModules(ObjIndex).Name
             Ok = m_CurrProj_ObjOpen(acModule, ObjName, ObjWasOpen, obj)
             p_Info InfoTxt & ObjName
             If Not SearchContinue Then Exit For
            
             If Ok Then
                 p_ScanModule obj
                 If ObjWasOpen = False Then
                     DoCmd.Close acModule, ObjName
                 End If
             Else
                 p_AddInList NoPermLst, NoPermNbr, c_Mod, ObjName, E_Txt_NoPermission.Caption
             End If
            
             p_ReplaceObjectName acModule, ObjName
            
         Next ObjIndex
        
         Set obj = Nothing
        
     End If
    
'    Search through CommandBars
'    --------------------------
     If (C_Search_CommandBars Or C_Search_UserCommandBars) And SearchContinue Then
        
         Dim oCB  As Object
         Dim oCBC As control
        
         InfoTxt = m_GetText("Db_SearchThrougthObjects", "E_C_CommandBars") & ": "
        
         For Each oCB In CommandBars
            
             DoEvents
             If Not SearchContinue Then Exit For
            
             If (oCB.BuiltIn And C_Search_CommandBars) Or ((Not oCB.BuiltIn) And C_Search_UserCommandBars) Then
                 p_Info InfoTxt & oCB.Name
                 p_ScanCmdBarCtr oCB, oCB.Name, oCB.BuiltIn
                 p_ScanCmdBar oCB
             End If
            
         Next oCB
        
         Set oCB = Nothing
         Set oCBC = Nothing
        
     End If
    
     p_Info vbNullString
    
End Sub

Private Sub p_AddInList(ByRef Lst() As String, ByRef Nbr As Long, objType As Integer, ObjName As String, Optional ObjSub As String, Optional ObjProp As String)
    
       Dim Ok         As Boolean
       Dim ObjTypeLab As String
    
       ObjTypeLab = L_Txt_Objects.Column(0, objType) ' short name of the object's type
    
       If Nbr = 0 Then
           Ok = True
       Else
          Ok = False
          Ok = Ok Or (Lst(c_ObjType, (Nbr - 1)) <> ObjTypeLab)
          Ok = Ok Or (Lst(c_ObjName, (Nbr - 1)) <> ObjName)
          Ok = Ok Or (Lst(c_ObjSub, (Nbr - 1)) <> ObjSub)
          Ok = Ok Or (Lst(c_ObjProp, (Nbr - 1)) <> ObjProp)
      End If
    
      If Ok Then
          Nbr = Nbr + 1
          ReDim Preserve Lst(0 To (c_FindCol - 1), 0 To (Nbr - 1)) As String
          Lst(c_ObjType, (Nbr - 1)) = ObjTypeLab
          Lst(c_ObjName, (Nbr - 1)) = ObjName
          Lst(c_ObjSub, (Nbr - 1)) = ObjSub
          Lst(c_ObjProp, (Nbr - 1)) = ObjProp
          Lst(c_ObjOpenType, (Nbr - 1)) = vbNullString & objType
      End If
    
End Sub

Private Sub p_ScanProperties(obj As Object, objType As Integer, Optional ObjName As String, Optional ObjSub As String)
    
       Const dbText = 10
       Const dbMemo = 12
       Const dbDate = 8
    
       Dim prop As Object 'Property can be confused with ADODB.Property object
    
       On Error GoTo Err_Scan
'    On Error GoTo 0
    
      For Each prop In obj.Properties
          If (prop.Type = dbText) Or (prop.Type = dbMemo) Or (prop.Type = dbDate) Then
'            There is a bug  : Forms(i).Section(i).Properties("Tag").Type returs 8 (as dbDate), it should be 10 (as dbText).
              If p_Search(prop.Value) Then
                  p_AddInList ResultLst, ResultNbr, objType, ObjName, ObjSub, "." & prop.Name
                  p_ReplaceProperty prop
              End If
NextProp:
          End If
      Next prop
    
End_Scan:
    Exit Sub
    
Err_Scan:
      Select Case Err.Number
    Case 3219, 3251, 2185, 2186, 2196
'        No premission to read, or does not exist in this context
          Resume NextProp
      Case Else
          DoCmd.Echo True
          DoCmd.Hourglass False
          MsgBox Err.Number & " " & Err.Description
          Stop
      End Select
    
End Sub

Private Sub p_ScanCmdBar(obj As Object)
    
'    There is no 'Properties' collection with CommandBar objets, so we have to scan text properties one after one :(
    
       With obj
        
           If p_Search(.Name) Then
               p_AddInList ResultLst, ResultNbr, c_Bar, .Name, , ".Name"
               If C_Replace = True Then
                   On Error Resume Next
                  .Name = p_ReplaceString(.Name, SearchStr, ReplaceWithStr)
                  ReplaceNbr = ReplaceNbr + 1
                  On Error GoTo 0
              End If
          End If
        
          If p_Search(.NameLocal) Then
              p_AddInList ResultLst, ResultNbr, c_Bar, .Name, , ".NameLocal"
              If C_Replace = True Then
                  On Error Resume Next
                  .Caption = p_ReplaceString(.Caption, SearchStr, ReplaceWithStr)
                  ReplaceNbr = ReplaceNbr + 1
                  On Error GoTo 0
              End If
          End If
        
          If .BuiltIn Then
              If p_Search(.Context) Then
                  p_AddInList ResultLst, ResultNbr, c_Bar, .Name, , ".Context"
                  If C_Replace = True Then
                      On Error Resume Next
                      .Context = p_ReplaceString(.Context, SearchStr, ReplaceWithStr)
                      ReplaceNbr = ReplaceNbr + 1
                      On Error GoTo 0
                  End If
              End If
          End If
        
      End With
    
End Sub

Private Sub p_ScanCmdBarCtr(obj As Object, ComBar As String, IsBuiltIn As Boolean)
'    There is no 'Properties' collection with CommandBarControl objets, so we have to scan text properties one after one :(
    
       Dim i As Long
    
       On Error GoTo Err_ScanCBC
    
       i = 1
    
       Do While p_CmdBarCtr_CheckId(obj, i) = True
        
          With obj.accChild(i)
            
              If p_Search(.Caption) Then
                  p_AddInList ResultLst, ResultNbr, c_Bar, ComBar, .Caption, ".Caption"
                  If C_Replace = True Then
                      On Error Resume Next
                      .Caption = p_ReplaceString(.Caption, SearchStr, ReplaceWithStr)
                      ReplaceNbr = ReplaceNbr + 1
                      On Error GoTo Err_ScanCBC
                  End If
              End If
            
              If p_Search(.DescriptionText) Then
                  p_AddInList ResultLst, ResultNbr, c_Bar, ComBar, .Caption, ".DescriptionText"
                  If C_Replace = True Then
                      On Error Resume Next
                      .DescriptionText = p_ReplaceString(.DescriptionText, SearchStr, ReplaceWithStr)
                      ReplaceNbr = ReplaceNbr + 1
                      On Error GoTo Err_ScanCBC
                  End If
              End If
            
              If Not IsBuiltIn Then
                  If p_Search(.OnAction) Then
                      p_AddInList ResultLst, ResultNbr, c_Bar, ComBar, .Caption, ".OnAction"
                      If C_Replace = True Then
                          On Error Resume Next
                          .OnAction = p_ReplaceString(.OnAction, SearchStr, ReplaceWithStr)
                          ReplaceNbr = ReplaceNbr + 1
                          On Error GoTo Err_ScanCBC
                      End If
                  End If
              End If
            
              If p_Search(.Parameter) Then
                  p_AddInList ResultLst, ResultNbr, c_Bar, ComBar, .Caption, ".Parameter"
                  If C_Replace = True Then
                      On Error Resume Next
                      .Parameter = p_ReplaceString(.Parameter, SearchStr, ReplaceWithStr)
                      ReplaceNbr = ReplaceNbr + 1
                      On Error GoTo Err_ScanCBC
                  End If
              End If
            
              If p_Search(.Tag) Then
                  p_AddInList ResultLst, ResultNbr, c_Bar, ComBar, .Caption, ".Tag"
                  If C_Replace = True Then
                      On Error Resume Next
                      .Tag = p_ReplaceString(.Tag, SearchStr, ReplaceWithStr)
                      ReplaceNbr = ReplaceNbr + 1
                      On Error GoTo Err_ScanCBC
                  End If
              End If
            
              If p_Search(.HelpFile) Then
                  p_AddInList ResultLst, ResultNbr, c_Bar, ComBar, .Caption, ".HelpFile"
                  If C_Replace = True Then
                      On Error Resume Next
                      .HelpFile = p_ReplaceString(.HelpFile, SearchStr, ReplaceWithStr)
                      ReplaceNbr = ReplaceNbr + 1
                      On Error GoTo Err_ScanCBC
                  End If
              End If
            
              If p_Search(.ToolTipText) Then
                  p_AddInList ResultLst, ResultNbr, c_Bar, ComBar, .Caption, ".TooltipText"
                  If C_Replace = True Then
                      On Error Resume Next
                      .ToolTipText = p_ReplaceString(.ToolTipText, SearchStr, ReplaceWithStr)
                      ReplaceNbr = ReplaceNbr + 1
                      On Error GoTo Err_ScanCBC
                  End If
              End If
            
          End With
        
          p_ScanCmdBarCtr obj.accChild(i), ComBar, IsBuiltIn
        
          i = i + 1
        
      Loop
    
Fin_ScanCBC:
      SearchProcessing = False
    Exit Sub
    
Err_ScanCBC:
      If Err = 438 Then
'        Bug : sudenly, there is no controls in the object
'        Debug.Print ComBar, n, obj.accChildCount
     Else
         MsgBox Err & " " & Err.Description
     End If
     Resume Fin_ScanCBC
    
End Sub

Private Sub p_ScanModule(m As Module)
    
       Dim LigId  As Long
       Dim LigTxt As String
    
'    Module objects haven't got the .Propoerties collection :(
       If p_Search(m.Name) Then
           p_AddInList ResultLst, ResultNbr, c_Mod, m.Name, , ".Name"
'        p_ReplaceInSearch m.Name
       End If
    
'    Search in source line by line
      For LigId = 1 To m.CountOfLines
          LigTxt = m.Lines(LigId, 1)
          If p_Search(LigTxt) Then
              p_AddInList ResultLst, ResultNbr, c_Mod, m.Name, , m.ProcOfLine(LigId, 0)
              p_ReplaceModuleLine m, LigId, LigTxt
          End If
      Next LigId
    
End Sub

Private Sub p_ScanOpenMacro()
    
       Dim x As String
       Dim i As Long
    
       With oMac
           Do Until .EndOfMacro
            
               If p_Search(.CurrAct_Comment) Then
                   p_AddInList ResultLst, ResultNbr, c_Mac, .Name, "Action #" & .CurrAct_ArgNbr, "Comment"
              End If
            
              If p_Search(.CurrAct_Label) Then
                  p_AddInList ResultLst, ResultNbr, c_Mac, .Name, "Action #" & .CurrAct_ArgNbr, "Label"
              End If
            
              If p_Search(.CurrAct_Condition) Then
                  p_AddInList ResultLst, ResultNbr, c_Mac, .Name, "Action #" & .CurrAct_ArgNbr, "Condition"
              End If
            
              For i = 1 To .CurrAct_ArgNbr
                  If p_Search(.CurrAct_Parameter(i)) Then
                      p_AddInList ResultLst, ResultNbr, c_Mac, .Name, "Action #" & .CurrAct_ArgNbr, "Parameter #" & i
                  End If
              Next i
            
              .NextAction
            
          Loop
          .CloseMacro
      End With
    
End Sub

Private Function p_OpenInDesignMode(Item As Long)
    
       Dim objType   As Integer
       Dim ObjName   As String
       Dim ObjDetail As String
    
       objType = Val(p_ListElement(c_ObjOpenType, Item))
       ObjName = p_ListElement(c_ObjName, Item)
       ObjDetail = 0
    
      On Error Resume Next
    
      Select Case objType
    Case c_DB
          DoCmd.Minimize
          DoCmd.RunCommand acCmdDatabaseProperties
      Case c_Tbl
          DoCmd.Minimize
          DoCmd.OpenTable ObjName, acViewDesign
      Case c_Qry
          DoCmd.Minimize
          DoCmd.OpenQuery ObjName, acViewDesign
      Case c_Frm
          DoCmd.Minimize
          DoCmd.OpenForm ObjName, acDesign
'        ObjDetail = p_ListElement(c_ObjSub, Item)
'        Forms(ObjName).Controls(ObjDetail).InSelection = True
      Case c_Rpt
          DoCmd.Minimize
          DoCmd.OpenReport ObjName, acDesign
'        ObjDetail = p_ListElement(c_ObjSub, Item)
'        Forms(ObjName).Controls(ObjDetail).InSelection = True
      Case c_Mod
        
          DoCmd.Minimize
        
'        If it's a form's module, then the form must be opened first
          Dim ObjWasOpen As Boolean
          Dim obj As Object
          Dim Ok As Boolean
          Dim AccObjName As String
          Dim AccObjType As AcObjectType
        
          If Left(ObjName, 4) = "Form_" Then
              AccObjType = acForm
              AccObjName = Mid$(AccObjName, 5)
          ElseIf Left(ObjName, 7) = "Report_" Then
              AccObjType = acReport
              AccObjName = Mid$(AccObjName, 8)
          Else
              AccObjType = 0
          End If
        
          If AccObjType = 0 Then
              Ok = False
          Else
              Ok = m_CurrProj_ObjOpen(AccObjType, AccObjName, ObjWasOpen, obj)
          End If
        
'        Try to find the name of the function
          ObjDetail = p_ListElement(c_ObjProp, Item)
          If Left$(ObjDetail, 1) = "." Then
              ObjDetail = Mid$(ObjDetail, 2)
              If ObjDetail = "Name" Then ObjDetail = ""
          End If
        
'        Open the module
          DoCmd.OpenModule ObjName, ObjDetail
        
'        Close the form if applicable
          If (Ok = True) And (ObjWasOpen = False) Then DoCmd.Close AccObjType, AccObjName
        
      Case c_Mac
      Case c_Bar
      End Select
    
End Function

Private Function p_SelectOne()
'    This function enables to select only one object type in the list
    
       Dim Ctr    As control
       Dim ActCtr As String
    
       ActCtr = Screen.ActiveControl.Name
    
       For Each Ctr In Me.Controls
           If Ctr.Name Like "C_Search_*" Then
              If Ctr.enabled Then
                  Ctr.Value = (Ctr.Name = ActCtr)
              End If
          End If
      Next Ctr
    
End Function

Private Function p_ListElement(Col As Variant, Lig As Variant) As String
'    Return the element of the list depending if 'show permision' is selected or not.
    
       If C_ShowWhenNoPerm Then
           If Lig < NoPermNbr Then
'            Show object with no permission (to read) first
               p_ListElement = NoPermLst(Col, Lig)
           Else
'            After, come objects with permission
               p_ListElement = ResultLst(Col, Lig - NoPermNbr)
          End If
      Else
          p_ListElement = ResultLst(Col, Lig)
      End If
    
End Function

Private Function p_ReplaceProperty(prop As Object)
    
       On Error GoTo Err_ReplaceProperty
    
       If C_Replace = True Then
           prop.Value = p_ReplaceString(prop.Value, SearchStr, ReplaceWithStr)
           ReplaceNbr = ReplaceNbr + 1
       End If
    
End_ReplaceProperty:
    Exit Function
    
Err_ReplaceProperty:
      Resume End_ReplaceProperty
    
End Function

Private Function p_ReplaceFormatCondition(obj As Object, objType As Integer, ObjName As String, i As Long)
    
       On Error GoTo Err_ReplaceFormatCondition
    
       If C_Replace = True Then
           With obj.FormatConditions(i)
               .Modify .Type, .Operator, p_ReplaceString(.Expression1, SearchStr, ReplaceWithStr), p_ReplaceString(.Expression2, SearchStr, ReplaceWithStr)
           End With
           DoCmd.SelectObject objType, ObjName
           DoCmd.Save objType, ObjName
          ReplaceNbr = ReplaceNbr + 1
      End If
    
End_ReplaceFormatCondition:
    Exit Function
    
Err_ReplaceFormatCondition:
      MsgBox Err.Description
      Resume End_ReplaceFormatCondition
    
End Function

Private Function p_ReplaceModuleLine(m As Module, LigId As Long, LigTxt As String)
    
       On Error GoTo Err_ReplaceModuleLine
    
       If C_Replace = True Then
           LigTxt = p_ReplaceString(LigTxt, SearchStr, ReplaceWithStr)
           m.ReplaceLine LigId, LigTxt
       End If
    
End_ReplaceModuleLine:
    Exit Function
    
Err_ReplaceModuleLine:
      Resume End_ReplaceModuleLine
    
End Function

Private Function p_ReplaceObjectName(objType As Integer, ObjName As String)
    
       On Error GoTo Err_ReplaceObjectName
    
       If C_Replace = True Then
           If p_Search(ObjName) Then
               DoCmd.Rename p_ReplaceString(ObjName, SearchStr, ReplaceWithStr), objType, ObjName
           End If
       End If
    
End_ReplaceObjectName:
    Exit Function
    
Err_ReplaceObjectName:
      Resume End_ReplaceObjectName
    
End Function

Private Function p_ReplaceString(Txt As String, RplWhat As String, RplWith As String) As String
    
       Dim s As String
       Dim i As Integer
    
'    Initialisation
       s = Txt
       i = 1
    
       If Len(RplWhat) > 0 Then
'        Recherche
          Do Until i > Len(s)
              If Mid$(s, i, Len(RplWhat)) = RplWhat Then
                  s = Left$(s, i - 1) & RplWith & Mid$(s, i + Len(RplWhat))
                  i = i + Len(RplWith)
              Else
                  i = i + 1
              End If
          Loop
      End If
    
      p_ReplaceString = s
    
End Function

