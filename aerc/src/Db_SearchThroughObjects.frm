Private Function p_HasFormatConditions(obj As Object) As Boolean
    
       Dim x As Long
    
       p_HasFormatConditions = False
       On Error Resume Next
    
       If (obj.ControlType = acTextBox) Or (obj.ControlType = acComboBox) Then
'        Property FormatConditions is not suported by Access 2000
           x = obj.FormatConditions.Count ' Since Access 2007, (Obj.Prop="x") can return true if Prop doesn't exist
          If x > 0 Then
              p_HasFormatConditions = True
          End If
      End If
    
End Function

Private Function p_Search(ByVal Txt As Variant) As Boolean
    
       On Error Resume Next
       Txt = vbNullString & Txt ' string conversion
    
       Select Case SearchType
    Case c_SearchType_Contains:
           p_Search = (InStr(1, Txt, SearchStr, vbTextCompare) > 0)
       Case c_SearchType_Equal:
           p_Search = (vbNullString & Txt = SearchStr)
      Case c_SearchType_Wildcards:
          p_Search = (Txt Like SearchStr)
      End Select
    
End Function

