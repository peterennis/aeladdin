Private Function p_PrepareAction(Action As String)
    
       Dim Ctr       As control
       Dim Folder    As String
       Dim HKey      As Long
       Dim FirstPage As Long
    
'    Get pages visible or not visible
       FirstPage = -1
       For Each Ctr In Me.Controls ' We scan all controls of the Form because TabAction has no property Controls.
          If Ctr.ControlType = acPage Then
              If InStr(Ctr.Tag, Action) > 0 Then
'                Debug.Print "visible : " & Ctr.Name & " (" & Ctr.PageIndex & ")"
                  Ctr.visible = True
                  If (FirstPage = -1) Or (FirstPage > Ctr.PageIndex) Then
                      FirstPage = Ctr.PageIndex
                  End If
              Else
                  Ctr.visible = False
              End If
          End If
      Next Ctr
      If FirstPage <> -1 Then TabAction.Value = FirstPage
    
      If Action = c_Act_Install Then
        
'        Show current information
'        ------------------------
'        zzz    HKey = m_Reg_KeyOpen(c_vReg_LocalMachine, m_AccVer(cz_vReg_ProgPath), KEY_READ)
          HKey = m_Reg_KeyOpen(c_vReg_LocalMachine, cz_vReg_ProgPath, KEY_READ)
        
          O_CurrentLanguage = m_DefaultLanguage(HKey)
        
          Folder = m_ProgDir(HKey)
          If Folder = "" Then
              txt_ProgDir = p_DefaultDir()
              txt_DataDir = p_DefaultDir()
          Else
              txt_ProgDir = Folder
              txt_DataDir = m_DataMdb_Folder(HKey)
          End If
        
          If HKey <> 0 Then
              m_Reg_KeyClose HKey
          End If
        
      End If
    
      If Action = c_Act_Uninstall Then
          CB_DeleteData = False
          CB_DeleteData.enabled = m_DataMdb_HasData()
      End If
    
      If Action = c_Act_CheckVersion Then
        
          Dim AccPath As String
          Dim AccKey  As String
          Dim Lst     As Variant
          Dim i       As Integer
        
'        Search for an Access version compatible with the current version
'        NOT NEEDED - Only support Access 2010
'        zzz    AccPath = vbNullString
'        zzz    Lst = Split(c_vAcceptedAccessVersions, ";")
'        zzz    For i = UBound(Lst) To LBound(Lst) Step -1
'        zzz      If AccPath = vbNullString Then
'        zzz        AccKey = Replace(c_vReg_AccessCommand, c_vAccVer_Internal, m_AccVersNum("" & Lst(i), True))
'        zzz        AccPath = "" & m_Reg_ValueGetQuick(c_vReg_ClassesRoot, AccKey)
'        zzz      End If
'        zzz    Next i
        
'        zzz    If AccPath = "" Then
'        zzz      E_AccessVersion_1.Visible = True
'        zzz      OkToClose = True
'        zzz      B_Action.Tag = ""
'        zzz      B_Action.Enabled = False
'        zzz    Else
'        zzz      E_AccessVersion_2.Visible = True
'        zzz      B_Action.Tag = Replace(AccPath, "%1", CodeDb.Name)
'        zzz      B_Action.Enabled = True
'        zzz    End If
        
      End If
    
      If gblnLOG Then
          Open gstr_LOGFILE For Append As #1
          Print #1, Date, "Now=" & Now, "p_PrepareAction"
          Print #1, , "Action=" & Action
          Print #1, , "FirstPage=" & FirstPage
          Print #1, , "txt_ProgDir=" & txt_ProgDir
          Print #1, , "txt_DataDir=" & txt_DataDir
          Print #1, , "TabAction.Value=" & TabAction.Value
          Close #1
      End If
    
      p_RefreshButtons
    
End Function

Private Function p_RefreshButtons()
    
       Dim ePrev As Boolean
       Dim eNext As Boolean
       Dim Ctr As control
    
       If gblnLOG Then
           Open gstr_LOGFILE For Append As #1
           Print #1, Date, "Now=" & Now, "p_RefreshButtons"
           Close #1
      End If
    
      For Each Ctr In Me.Controls ' We scan all controls of the Form because TabAction has no property Controls.
          If Ctr.ControlType = acPage Then
              If Ctr.visible = True Then
                  ePrev = ePrev Or (Ctr.PageIndex < TabAction.Value)
                  eNext = eNext Or (Ctr.PageIndex > TabAction.Value)
              End If
          End If
      Next Ctr
    
      If B_Cancel.enabled And B_Cancel.visible Then
          B_Cancel.SetFocus
      Else
          B_Action.SetFocus
      End If
    
      If (Not ePrev) And (Not eNext) Then
          B_Previous.visible = False
          B_Next.visible = False
      Else
          B_Previous.visible = True
          B_Previous.enabled = ePrev
          B_Next.visible = True
          B_Next.enabled = eNext
          B_Action.enabled = (Not eNext)
      End If
    
End Function

