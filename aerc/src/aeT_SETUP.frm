           Print #1, Date, "Now=" & Now, "p_Act_Choose"
           Close #1
       End If
    
      Select Case O_Action
        
    Case O_Act_Uninstall.OptionValue
        
'        In order to delete files and directories, this program has to restart from Temp directory
          Dim Src  As String
          Dim Dest As String
          Dim i    As Integer
          Dim Cmd  As String
        
'        Copy the current program into the Windows temp directory
          Src = CodeDb.Name
'        zzz            Dest = m_Folder_CheckEnd(p_Folder_GetTempPath(), True) & m_AccVer(cz_vProgFile) & m_AccExt()
          Dest = m_Folder_CheckEnd(p_Folder_GetTempPath(), True) & cz_vProgFile & m_AccExt()
          If Dir$(Dest) <> "" Then
              m_File_Delete Dest
          End If
          m_File_Copy Src, Dest
        
'        Restart the program from the Windows temp directory
          Cmd = m_Folder_CheckEnd(SysCmd(acSysCmdAccessDir), True) & "MSACCESS.EXE """ & Dest & """ /cmd " & c_Act_Uninstall
          Shell Cmd, vbMaximizedFocus
        
'        Close this program, another cession is already lunched
          p_Close
        
      Case O_Act_Setup.OptionValue, O_Act_Upgrade.OptionValue
        
          p_PrepareAction c_Act_Install
        
      End Select
    
      If gblnLOG Then
          Open gstr_LOGFILE For Append As #1
          Print #1, Date, "Now=" & Now, "END> p_Act_Choose"
          Close #1
      End If
    
      On Error GoTo 0
    Exit Sub
    
p_Act_Choose_Error:
    
      MsgBox "Erl=" & Erl & " Error " & Err.Number & " (" & Err.Description & ") in procedure p_Act_Choose of VBA Document Form_aeT_SETUP"
    
End Sub

Private Sub p_Act_Uninstall()
    
       Dim ProgDir  As String
       Dim ProgFile As String
       Dim DataFile As String
       Dim HKey     As Long
       Dim NoSubKey As Boolean
    
       On Error GoTo p_Act_Uninstall_Error
    
      If gblnLOG Then
          Open gstr_LOGFILE For Append As #1
          Print #1, Date, "Now=" & Now, "p_Act_Uninstall"
          Close #1
      End If
    
'    Retrieve progam files and directories to delete
'    zzz    HKey = m_Reg_KeyOpen(c_vReg_LocalMachine, m_AccVer(cz_vReg_ProgPath))
      HKey = m_Reg_KeyOpen(c_vReg_LocalMachine, cz_vReg_ProgPath)
      If HKey = 0 Then
          MsgBox "Error: the registry was expected to be correctly filled.", vbCritical
          p_Close
      Else
          ProgDir = m_ProgDir(HKey)
'        zzz        ProgFile = m_Folder_CheckEnd(ProgDir, True) & m_AccVer(cz_vProgFile) & m_AccExt()
          ProgFile = m_Folder_CheckEnd(ProgDir, True) & cz_vProgFile & m_AccExt()
          DataFile = m_DataMdb_FullPath(HKey)
          m_Reg_KeyClose HKey
      End If
    
'    Delete files
      E_Uninstall_01.FontBold = True
      m_File_Delete ProgFile
      If (Not m_DataMdb_HasData(DataFile)) Or CB_DeleteData Then
          m_File_Delete DataFile
      End If
'    zzz    DataFile = m_Folder_CheckEnd(ProgDir, True) & m_AccVer(cz_vDataFile) & m_AccExt()
      DataFile = m_Folder_CheckEnd(ProgDir, True) & cz_vDataFile & m_AccExt()
      If (Not m_DataMdb_HasData(DataFile)) Or CB_DeleteData Then
          m_File_Delete DataFile
      End If
    
'    Delete directories (if there is still som files or subdir, there will be no detetation
      On Error Resume Next
      RmDir m_Folder_CheckEnd(ProgDir, False)
      ProgDir = m_File_GetFolder(m_Folder_CheckEnd(ProgDir, False)) 'parent directory
      RmDir m_Folder_CheckEnd(ProgDir, False)
      On Error GoTo 0
    
'    Delete keys about add-in menu
      E_Uninstall_02.FontBold = True
      If p_DeleteOldMenus() = False Then
          If gblnLOG Then
              Open gstr_LOGFILE For Append As #1
              Print #1, , "p_Act_Uninstall: RegWriteError, VBCRITICAL EXIT-1"
              Close #1
          End If
          m_MsgBox "Sys", "RegWriteError", vbCritical
          p_Close
        Exit Sub
      End If
    
'    Delete registry information
      E_Uninstall_03.FontBold = True
      If CB_DeleteReg Then
'        Delete the program key
'        zzz        HKey = m_Reg_KeyOpen(c_vReg_LocalMachine, m_AccVer(cz_vReg_EditorPath))
          HKey = m_Reg_KeyOpen(c_vReg_LocalMachine, cz_vReg_EditorPath)
          If HKey <> 0 Then
'            zzz            m_Reg_KeyDelete HKey, m_AccVer(cz_vReg_ProgKey)
              m_Reg_KeyDelete HKey, cz_vReg_ProgKey
              NoSubKey = (m_Reg_KeyEnum(HKey, 0) = "")
              m_Reg_KeyClose HKey
          End If
'        Delete the editor key is no sub-key
          If NoSubKey Then
              HKey = m_Reg_KeyOpen(c_vReg_LocalMachine, c_vReg_RootKey)
              If HKey <> 0 Then
                  m_Reg_KeyDelete HKey, c_vEditor
                  m_Reg_KeyClose HKey
              End If
          End If
      End If
'    Delete uninstall key
'    zzz    HKey = m_Reg_KeyOpen(c_vReg_LocalMachine, m_AccVer(cz_vReg_Uninstall))
      HKey = m_Reg_KeyOpen(c_vReg_LocalMachine, cz_vReg_Uninstall)
      If HKey <> 0 Then
'        zzz        m_Reg_KeyDelete HKey, m_AccVer(cz_vFullName)
          m_Reg_KeyDelete HKey, cz_vFullName
'        zzz        m_Reg_KeyDelete HKey, Replace(m_AccVer(cz_vFullName), " (", "(") 'error in the name of the key in previous versions (from 1.50 to 1.52)
          m_Reg_KeyClose HKey
      End If
    
'    Ask the system to delete the current program (mdb) when restart
      p_MoveFileCurrentlyInUse CodeDb.Name, ""
    
      m_MsgBox Me.Name, "UninstallEnd", vbInformation
      p_Close
    
      If gblnLOG Then
         Open gstr_LOGFILE For Append As #1
         Print #1, Date, "Now=" & Now, "END> p_Act_Uninstall"
         Close #1
     End If
    
     On Error GoTo 0
    Exit Sub
    
p_Act_Uninstall_Error:
    
     MsgBox "Error " & Err.Number & " (" & Err.Description & ") in procedure p_Act_Uninstall of VBA Document Form_aeT_SETUP"
    
End Sub

Private Function f_FoldersOK() As Boolean
    
       Dim FolderOk1 As Boolean
       Dim FolderOk2 As Boolean
    
       On Error GoTo f_FoldersOK_Error
    
       If gblnLOG Then
           Open gstr_LOGFILE For Append As #1
           Print #1, Date, "Now=" & Now, "f_FoldersOK"
          Print #1, , "txt_ProgDir=" & txt_ProgDir
          Print #1, , "txt_DataDir=" & txt_DataDir
          Close #1
      End If
    
'    Verify folders
'    ---------------
      FolderOk1 = p_Folder_Check("" & txt_ProgDir)
      If FolderOk1 = False Then
          If m_MsgBox(Me.Name, "CreateFolder", vbDefaultButton2 + vbQuestion + vbOKCancel, txt_ProgDir) <> vbOK Then
            Exit Function
          End If
      End If
      If ("" & txt_ProgDir) = ("" & txt_DataDir) Then
          FolderOk2 = True
      Else
          FolderOk2 = p_Folder_Check("" & txt_DataDir)
          If FolderOk2 = False Then
              If m_MsgBox(Me.Name, "CreateFolder", vbDefaultButton2 + vbQuestion + vbOKCancel, txt_DataDir) <> vbOK Then
                Exit Function
              End If
          End If
      End If
    
      DoCmd.Hourglass True
    
'    Create missing Folders
'    ------------------------
      If FolderOk1 = False Then
          FolderOk1 = p_Folder_Check("" & txt_ProgDir, True)
          If FolderOk1 = False Then
            Exit Function
          End If
      End If
      If FolderOk2 = False Then
          FolderOk2 = p_Folder_Check("" & txt_DataDir, True)
          If FolderOk2 = False Then
            Exit Function
          End If
      End If
    
      If gblnLOG Then
          Open gstr_LOGFILE For Append As #1
          Print #1, , "FolderOk1=" & FolderOk1
          Print #1, , "FolderOk2=" & FolderOk2
          Close #1
      End If
    
      If FolderOk1 And FolderOk2 Then
          f_FoldersOK = True
      Else
          f_FoldersOK = False
      End If
    
      If gblnLOG Then
          Open gstr_LOGFILE For Append As #1
          Print #1, Date, "Now=" & Now, "END> f_FoldersOK"
          Close #1
      End If
    
      On Error GoTo 0
    Exit Function
    
f_FoldersOK_Error:
    
      MsgBox "Erl=" & Erl & " Error " & Err.Number & " (" & Err.Description & ") in procedure f_FoldersOK of VBA Document Form_aeT_SETUP"
    
End Function

Private Sub p_Act_Install()
    
       Dim File1     As String
       Dim Ok        As Boolean
