Private Function p_Folder_GetTempPath() As String
    
       Const MAX_PATH = 260
    
       Dim strFolder As String
       Dim lngResult As Long
    
       If gblnLOG Then
           Open gstr_LOGFILE For Append As #1
           Print #1, Date, "Now=" & Now, "p_Folder_GetTempPath"
          Close #1
      End If
    
      strFolder = String(MAX_PATH, 0)
      lngResult = api_GetTempPath(MAX_PATH, strFolder)
    
      If lngResult <> 0 Then
          p_Folder_GetTempPath = Left(strFolder, InStr(strFolder, Chr$(0)) - 1)
      Else
          p_Folder_GetTempPath = ""
      End If
    
End Function

