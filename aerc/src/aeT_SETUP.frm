'        zzz        m_Reg_ValueSet HKey, "DisplayName", m_AccVer(cz_vFullName), REG_SZ
          m_Reg_ValueSet HKey, "DisplayName", cz_vFullName, REG_SZ
          m_Reg_ValueSet HKey, "UninstallString", CmdStr, REG_SZ
          m_Reg_KeyClose HKey
      End If
    
'    zzz  ' Debug an old bug
'    zzz  HKey = m_Reg_KeyOpen(c_vReg_LocalMachine, m_AccVer(cz_vReg_Uninstall))
'    zzz  If HKey <> 0 Then
'    zzz    m_Reg_KeyDelete HKey, Replace(m_AccVer(cz_vFullName), " (", "(") 'error in the name of the key in previous versions (from 1.50 to 1.52)
'    zzz    m_Reg_KeyClose HKey
'    zzz  End If
    
    
      DoCmd.Hourglass False
    
      m_MsgBox Me.Name, "InstallEnd", vbInformation
    
      p_Close
    
      If gblnLOG Then
          Open gstr_LOGFILE For Append As #1
          Print #1, Date, "Now=" & Now, "END> p_Act_Install"
          Close #1
      End If
    
     On Error GoTo 0
    Exit Sub
    
p_Act_Install_Error:
    
     MsgBox "Erl=" & Erl & " Error " & Err.Number & " (" & Err.Description & ") in procedure p_Act_Install of VBA Document Form_aeT_SETUP"
