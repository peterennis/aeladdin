Private Sub B_Action_Click()
    
       On Error GoTo B_Action_Click_Error
    
       If gblnLOG Then
           Open gstr_LOGFILE For Append As #1
           Print #1, Date, "Now=" & Now, "B_Action_Click"
           Print #1, , "TabAction.Value=" & TabAction.Value
           Print #1, , "P_Uninstall.PageIndex=" & P_Uninstall.PageIndex
           Print #1, , "P_Choose.PageIndex=" & P_Choose.PageIndex
          Print #1, , "P_Path.PageIndex=" & P_Path.PageIndex
          Print #1, , "P_CheckVersion.PageIndex=" & P_CheckVersion.PageIndex
          Close #1
      End If
    
      Select Case TabAction.Value
    Case P_Uninstall.PageIndex
          If m_MsgBox(Me.Name, "UninstallConf", vbOKCancel + vbDefaultButton2 + vbExclamation) = vbOK Then
              p_Act_Uninstall
          Else
              p_Close
          End If
      Case P_Choose.PageIndex
          p_Act_Choose
      Case P_Path.PageIndex
          p_Act_Install
      Case P_CheckVersion.PageIndex
          If B_Action.Tag <> "" Then
              Shell B_Action.Tag
          End If
          p_Close
      End Select
    
      If gblnLOG Then
          Open gstr_LOGFILE For Append As #1
          Print #1, Date, "Now=" & Now, "END> B_Action_Click"
          Close #1
      End If
    
      On Error GoTo 0
    Exit Sub
    
B_Action_Click_Error:
    
      MsgBox "Erl=" & Erl & " Error " & Err.Number & " (" & Err.Description & ") in procedure B_Action_Click of VBA Document Form_aeT_SETUP"
    
End Sub

