      img_btn_en.visible = True
'    zzz  img_btn_Fr.Visible = True
'    zzz  img_btn_De.Visible = True
'    zzz  img_btn_pt.Visible = True
    
      B_Cancel.visible = True
      B_Previous.visible = True
      B_Next.visible = True
      B_Action.visible = True
    
      TabAction.Style = 2
      TabAction.BackStyle = 0
      TabAction.Top = 0
      TabAction.Height = Me.Section(0).Height
      p_AppActivate
    
      Select Case Command()
    Case c_Act_Uninstall
        
          p_PrepareAction c_Act_Uninstall
        
      Case Else
        
'        zzz            ' Check if the current Access version is the expected version
'        zzz            ' -----------------------------------------------------------
'        zzz            If InStr(c_vAcceptedAccessVersions, SysCmd(acSysCmdAccessVer)) = 0 Then
'        zzz                p_PrepareAction c_Act_CheckVersion
'        zzz                Exit Sub
'        zzz            End If
        
'        Check for the current version
          Select Case Val(RegVersion)
        Case 0
'            The key is not registered -> normal setup
              p_DirControls True
              p_PrepareAction c_Act_Install
          Case Is > Val(c_vVersion)
'            The registered version is a newest version -> message
              m_MsgBox Me.Name, "aeVersionCheck", vbExclamation
              p_Close
          Case Else
'            Choose between Uninstall/Reinstall/Upgrade
              p_DirControls False
              If Val(RegVersion) = Val(c_vVersion) Then
                  O_Act_Upgrade.enabled = False
'                If the installed VTools is already in use, then it cannot be reinstalled.
