      If gblnLOG Then
          Open gstr_LOGFILE For Append As #1
          Print #1, Date, "Now=" & Now, "p_Act_Install"
          Close #1
      End If
    
'    Delete old menus and check if the current user has enough permission for writing to the registry
      If p_DeleteOldMenus() = False Then
          If gblnLOG Then
              Open gstr_LOGFILE For Append As #1
              Print #1, Date, "Now=" & Now, "p_Act_Install: RegWriteError VBCRITICAL EXIT-1"
              Close #1
          End If
          m_MsgBox "Sys", "RegWriteError", vbCritical
          DoCmd.Quit acQuitSaveNone
'        p_Close
'        Exit Sub
      End If
    
      If f_FoldersOK Then
      Else
          If gblnLOG Then
