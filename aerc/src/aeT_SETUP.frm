                      Me.O_Act_Setup.enabled = False
                  End If
              Else
                  O_Act_Setup.enabled = False
              End If
              p_PrepareAction c_Act_Choose
          End Select
        
      End Select
    
      If gblnLOG Then
          Open gstr_LOGFILE For Append As #1
          Print #1, Date, "Now=" & Now, "END> " & Me.Name & " Form_Load"
          Close #1
      End If
    
      On Error GoTo 0
