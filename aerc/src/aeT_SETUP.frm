Private Function p_DefaultDir() As String
'    Returns the default 'Program Files' folder
    
       If gblnLOG Then
           Open gstr_LOGFILE For Append As #1
           Print #1, Date, "Now=" & Now, "p_DefaultDir"
           Close #1
       End If
    
       If DefaultDir = "" Then
          DefaultDir = m_Reg_ValueGetQuick(c_vReg_LocalMachine, "Software\Microsoft\Windows\CurrentVersion", "ProgramFilesPath")
          If (DefaultDir = vbNullString) Or (InStr(DefaultDir, "%") > 0) Then
'            Key ProgramFilesPath may contain environement string like %ProgramFilesDir%, but such environement strings is automatically replaced by the API when the key is read.
'            Nevertheless, a bug has been reported that ProgramFilesPath has an invalide value in some cases, so we try the following for saving the situation
              DefaultDir = m_Reg_ValueGetQuick(c_vReg_LocalMachine, "Software\Microsoft\Windows\CurrentVersion", "ProgramFilesDir")
          End If
'        zzz    DefaultDir = DefaultDir & m_AccVer(cz_vDefaultProgDir)
          DefaultDir = DefaultDir & cz_vDefaultProgDir
'        The path can be registred with '\' or '\\' separator
          DefaultDir = Replace(DefaultDir, "\\", "\")
          If Left$(DefaultDir, 1) = "\" Then
'            Network path
              DefaultDir = "\" & DefaultDir
          End If
      End If
    
      p_DefaultDir = DefaultDir
    
End Function

Private Function p_Folder_Check(FolderPath As String, Optional Create As Boolean) As Boolean
'    This function checks if the folder and sub folders does exist.
    
       If gblnLOG Then
           Open gstr_LOGFILE For Append As #1
           Print #1, Date, "Now=" & Now, "p_Folder_Check"
           Close #1
       End If
    
       If Create Then
        
          Const c_Delim = "\"
        
          Dim Folder     As String
          Dim Folders    As Variant
          Dim iMax       As Integer
        
          Dim CurrErr        As Long
          Dim LastErrNum     As Long
          Dim LastErrFolder  As String
        
          Dim i          As Integer
        
          Folders = Split(FolderPath, c_Delim)
          Folder = ""
          iMax = UBound(Folders)
        
          For i = 0 To iMax
              Folder = Folder & Folders(i) & c_Delim
              If Folders(i) <> "" Then
'                ("" folder occurs when a network path is specified, ex : \\srv\fld1\fld2)
                  CurrErr = p_Folder_Create(Folder)
                  If CurrErr <> LastErrNum Then
                      LastErrNum = CurrErr
                      LastErrFolder = Folder
                  End If
              End If
          Next i
        
      End If
    
      p_Folder_Check = p_Folder_IsExisting(FolderPath)
    
'    If an error has occured during the folder creation, the error is prompted
      If Create And (Not p_Folder_Check) Then
          MsgBox "'" & LastErrFolder & "'" & vbNewLine & Error(LastErrNum), vbCritical, Me.Caption
      End If
    
End Function

Private Function p_DeleteOldMenus() As Boolean
'    Delete all the add-in Menus (those that begins with the application's prefix)
    
       On Error GoTo p_DeleteOldMenus_Error
    
       gstr_LOGFILE = f_MyDocuments & "\" & c_LOGFILE & Format(Date, "yyyymmdd") & ".txt"
    
       If gblnLOG Then
           Open gstr_LOGFILE For Append As #1
           Print #1, Date, "Now=" & Now, "p_DeleteOldMenus"
          Print #1, , "cz_vReg_AccessAddIns=" & cz_vReg_AccessAddIns
          Print #1, , "cz_vReg_AccessAddIns=" & cz_vReg_AccessAddIns
          Print #1, , "c_vReg_LocalMachine=" & c_vReg_LocalMachine
          Print #1, , "HKey=" & m_Reg_KeyOpen(c_vReg_LocalMachine, cz_vReg_AccessAddIns)
          Close #1
      End If
    
      Dim i        As Long
      Dim Men()    As String
      Dim MenNbr   As Long
      Dim HKey     As Long
      Dim x        As String
      Dim Finished As Boolean
    
'    zzz    HKey = m_Reg_KeyOpen(c_vReg_LocalMachine, m_AccVer(cz_vReg_AccessAddIns))
      HKey = m_Reg_KeyOpen(c_vReg_LocalMachine, cz_vReg_AccessAddIns)
    
      If HKey <> 0 Then
          i = 1
          MenNbr = 0
          Finished = False
        
'        Save in the array the key that has to be deleted
          Do Until (Finished = True)
              x = m_Reg_KeyEnum(HKey, i)
              If x = "" Then
                  Finished = True
              Else
                  If (x Like c_vMenuPrefixe0 & "*") Or (x Like c_vMenuPrefixe1 & "*") Then
                      MenNbr = MenNbr + 1
                      ReDim Preserve Men(1 To MenNbr)
                      Men(MenNbr) = x
                  End If
              End If
              i = i + 1
          Loop
        
'        Now delete all key in the array
          For i = 1 To MenNbr
              m_Reg_KeyDelete HKey, Men(i)
          Next i
        
'        Close the key
          m_Reg_KeyClose HKey
        
          p_DeleteOldMenus = True
        
      End If
    
      If gblnLOG Then
          Open gstr_LOGFILE For Append As #1
          Print #1, , "p_DeleteOldMenus=" & p_DeleteOldMenus
          Print #1, Date, "Now=" & Now, "END> p_DeleteOldMenus"
          Close #1
      End If
    
      On Error GoTo 0
    Exit Function
    
p_DeleteOldMenus_Error:
    
      MsgBox "Erl=" & Erl & " Error " & Err.Number & " (" & Err.Description & ") in procedure p_DeleteOldMenus of VBA Document Form_aeT_SETUP"
    
End Function

Private Function p_MoveFileCurrentlyInUse(FileSrc As String, FileTrg As String) As Boolean
'    Move a file that is current in use.
'    This can be performed by asking the system to rembemebr to move the file when it will reboot.
'    To delete the file, specify FileTrg=""
'    In WinNT, directories can be moved/deleted with this function pout not in Win9x.
    
       If gblnLOG Then
           Open gstr_LOGFILE For Append As #1
           Print #1, Date, "Now=" & Now, "p_MoveFileCurrentlyInUse"
           Close #1
      End If
    
      Const c_Exe = "Wininit.exe"
      Const c_Del = ""
    
      Dim SysDir As String
      SysDir = "" & m_Reg_ValueGetQuick(c_vReg_LocalMachine, "Software\Microsoft\Windows\CurrentVersion", "SystemRoot")
      If SysDir = "" Then
          SysDir = m_Reg_ExpandString("%SystemRoot%")
      End If
      SysDir = m_Folder_CheckEnd(SysDir, True)
    
      If m_File_IsExisting(SysDir & c_Exe) Then
        
'        Win95 system must use Wininit.exe to delete the file.
'        We have to inster the IniStr line into the Wininit.ini file.
'        But the WritePrivateProfileString() can't be used because ther can be several NUL=... entries under the [rename] bloc.
        
          Const c_Ini = "Wininit.ini"
          Const c_Tag = "[rename]"
        
          Dim IniNum As Integer
          Dim IniTxt As String
          Dim IniLig As String
          Dim IniStr As String
          Dim IniOk  As Boolean
        
'        Initialise variables
          IniOk = False
          IniStr = IIf(FileTrg = c_Del, "NUL", FileTrg) & "=" & p_GetShortPathName(FileSrc)
        
'        Put the existing ini lines into the IniTxt variable
          If m_File_IsExisting(SysDir & c_Ini) Then
            
              IniNum = FreeFile()
              Open (SysDir & c_Ini) For Input Access Read As #IniNum
            
              Do Until EOF(IniNum)
                  Line Input #IniNum, IniLig
                  If Trim$(IniLig) = c_Tag Then
                      IniLig = IniLig & vbNewLine & IniStr
                      IniOk = True
                  End If
                  If IniTxt = "" Then
                      IniTxt = IniLig
                  Else
                      IniTxt = IniTxt & vbNewLine & IniLig
                  End If
              Loop
            
              Close #IniNum
            
          End If
        
'        If the ini files doesn't existe or if the bloc was not in the ini file, we add it in the IniStr variable
          If Not IniOk Then
              If IniTxt <> "" Then
                  IniTxt = IniTxt & vbNewLine & vbNewLine
              End If
              IniTxt = IniTxt & c_Tag & vbNewLine & IniStr
          End If
        
'        We write down the IniStr into the ini file.
          IniNum = FreeFile()
          Open (SysDir & c_Ini) For Output Access Write As #IniNum
          Print #IniNum, IniTxt
          Close #IniNum
        
          p_MoveFileCurrentlyInUse = True
        
      Else
        
          Const MOVEFILE_DELAY_UNTIL_REBOOT = &H4
        
          Dim ret    As Long
        
'        WinNT and Win2000 system can use the MoveFileEx API that enable to move a file after the reboot
          ret = api_MoveFileEx(FileSrc, IIf(FileTrg = c_Del, vbNull, """" & FileTrg & """"), MOVEFILE_DELAY_UNTIL_REBOOT)
          p_MoveFileCurrentlyInUse = (ret <> 0)
        
      End If
    
End Function

