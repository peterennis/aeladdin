Private Function p_ReplaceString(Txt As String, RplWhat As String, RplWith As String) As String
    
2       Dim s As String
3       Dim i As Integer
    
'    Initialisation
6       s = Txt
7       i = 1
    
9       If Len(RplWhat) > 0 Then
'        Recherche
11          Do Until i > Len(s)
12              If Mid$(s, i, Len(RplWhat)) = RplWhat Then
13                  s = Left$(s, i - 1) & RplWith & Mid$(s, i + Len(RplWhat))
14                  i = i + Len(RplWith)
