Private Sub B_CopyToClipboard_Click()
    
2       Dim Result As String
3       Dim Col    As Integer
4       Dim Col0   As Integer
5       Dim ColN   As Integer
6       Dim Row    As Long
7       Dim str    As String
    
9       Col0 = LBound(ResultLst, 1)
10      ColN = UBound(ResultLst, 1)
11      Result = ""
    
13      For Row = LBound(ResultLst, 2) To UBound(ResultLst, 2)
        
15          str = ""
        
17          For Col = Col0 To ColN
18              If str = "" Then
19                  If Result <> "" Then
20                      str = vbCrLf
21                  End If
22              Else
23                  str = str & vbTab
24              End If
25              str = str & ResultLst(Col, Row)
26          Next Col
        
28          Result = Result & str
        
