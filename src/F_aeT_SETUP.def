Private Function p_MoveFileCurrentlyInUse(FileSrc As String, FileTrg As String) As Boolean
'    Move a file that is current in use.
'    This can be performed by asking the system to rembemebr to move the file when it will reboot.
'    To delete the file, specify FileTrg=""
'    In WinNT, directories can be moved/deleted with this function pout not in Win9x.
    
6       If gblnLOG Then
7           Open gstr_LOGFILE For Append As #1
8           Print #1, Date, "Now=" & Now, "p_MoveFileCurrentlyInUse"
9           Close #1
10      End If
    
12      Const c_Exe = "Wininit.exe"
13      Const c_Del = ""
    
15      Dim SysDir As String
16      SysDir = "" & m_Reg_ValueGetQuick(c_vReg_LocalMachine, "Software\Microsoft\Windows\CurrentVersion", "SystemRoot")
17      If SysDir = "" Then
18          SysDir = m_Reg_ExpandString("%SystemRoot%")
19      End If
20      SysDir = m_Folder_CheckEnd(SysDir, True)
    
22      If m_File_IsExisting(SysDir & c_Exe) Then
        
'        Win95 system must use Wininit.exe to delete the file.
'        We have to inster the IniStr line into the Wininit.ini file.
'        But the WritePrivateProfileString() can't be used because ther can be several NUL=... entries under the [rename] bloc.
        
28          Const c_Ini = "Wininit.ini"
29          Const c_Tag = "[rename]"
        
31          Dim IniNum As Integer
32          Dim IniTxt As String
33          Dim IniLig As String
34          Dim IniStr As String
35          Dim IniOk  As Boolean
        
'        Initialise variables
38          IniOk = False
39          IniStr = IIf(FileTrg = c_Del, "NUL", FileTrg) & "=" & p_GetShortPathName(FileSrc)
        
'        Put the existing ini lines into the IniTxt variable
42          If m_File_IsExisting(SysDir & c_Ini) Then
            
44              IniNum = FreeFile()
45              Open (SysDir & c_Ini) For Input Access Read As #IniNum
            
47              Do Until EOF(IniNum)
48                  Line Input #IniNum, IniLig
49                  If Trim$(IniLig) = c_Tag Then
50                      IniLig = IniLig & vbNewLine & IniStr
51                      IniOk = True
52                  End If
53                  If IniTxt = "" Then
54                      IniTxt = IniLig
55                  Else
56                      IniTxt = IniTxt & vbNewLine & IniLig
57                  End If
58              Loop
            
60              Close #IniNum
            
62          End If
        
'        If the ini files doesn't existe or if the bloc was not in the ini file, we add it in the IniStr variable
65          If Not IniOk Then
66              If IniTxt <> "" Then
67                  IniTxt = IniTxt & vbNewLine & vbNewLine
68              End If
69              IniTxt = IniTxt & c_Tag & vbNewLine & IniStr
70          End If
        
'        We write down the IniStr into the ini file.
73          IniNum = FreeFile()
74          Open (SysDir & c_Ini) For Output Access Write As #IniNum
75          Print #IniNum, IniTxt
76          Close #IniNum
        
78          p_MoveFileCurrentlyInUse = True
        
80      Else
        
82          Const MOVEFILE_DELAY_UNTIL_REBOOT = &H4
        
84          Dim ret    As Long
        
'        WinNT and Win2000 system can use the MoveFileEx API that enable to move a file after the reboot
87          ret = api_MoveFileEx(FileSrc, IIf(FileTrg = c_Del, vbNull, """" & FileTrg & """"), MOVEFILE_DELAY_UNTIL_REBOOT)
88          p_MoveFileCurrentlyInUse = (ret <> 0)
        
90      End If
    
End Function

